<% Quiz.Questions.forEach((question, index) => { %>
   <% let questionNum = index + 1; %>
   <% let questionType = Object.values(Quiz.Types.Buttons).includes(question.question_type) ? "buttonselect" : question.question_type; %>
      <div id=<%="generated-quiz-modal-question"+questionNum+"-container"%> style="z-index: 1;">
         <% if (questionType=='buttonselect') { %>
            <%- include("./button_select",{getText, question}) %>  
         <% } else if (questionType==Quiz.Types.RangeSlider) { %>
             <%- include("./range_slider",{getText, question}) %>  
            <%} else if (questionType==Quiz.Types.TimeRange){ %>
            <%- include("./time_picker",{getText,question}) %>
            <%} 
         %>
      </div>
 <% }); %>